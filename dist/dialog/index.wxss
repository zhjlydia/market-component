@import '../common/style/index.wxss';.spui-dialog{width:85%;font-size:16px;overflow:hidden;border-radius:4px;background-color:#fff}.spui-dialog__header{font-weight:500;padding-top:25px;text-align:center}.spui-dialog__header--isolated{padding:25px 0}.spui-dialog__message{padding:25px;font-size:14px;line-height:1.5;max-height:60vh;overflow-y:auto;text-align:center;-webkit-overflow-scrolling:touch}.spui-dialog__message--has-title{padding-top:12px;color:#7d7e80}.spui-dialog__message--left{text-align:left}.spui-dialog__message--right{text-align:right}.spui-dialog__footer{display:-webkit-box;display:flex}.spui-dialog__button{-webkit-box-flex:1;flex:1}.spui-dialog__cancel,.spui-dialog__confirm{border:0!important}.spui-dialog__confirm,.spui-dialog__confirm:active{color:#1989fa!important}.spui-dialog-bounce-enter{opacity:0;-webkit-transform:translate3d(-50%,-50%,0) scale(.7);transform:translate3d(-50%,-50%,0) scale(.7)}.spui-dialog-bounce-leave-active{opacity:0;-webkit-transform:translate3d(-50%,-50%,0) scale(.9);transform:translate3d(-50%,-50%,0) scale(.9)}