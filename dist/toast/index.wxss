@import '../common/style/index.wxss';.spui-toast{display:-webkit-box;display:flex;color:#fff;font-size:14px;line-height:20px;border-radius:4px;word-break:break-all;-webkit-box-align:center;align-items:center;-webkit-box-orient:vertical;-webkit-box-direction:normal;flex-direction:column;-webkit-box-pack:center;justify-content:center;box-sizing:content-box;background-color:rgba(0,0,0,.7)}.spui-toast__container{position:fixed;top:50%;left:50%;max-width:70%;width:-webkit-fit-content;width:-moz-fit-content;width:fit-content;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.spui-toast--text{padding:8px 12px;min-width:96px}.spui-toast--icon{width:90px;padding:15px;min-height:90px}.spui-toast--icon .spui-toast__icon{font-size:48px}.spui-toast--icon .spui-toast__text{padding-top:5px}.spui-toast__loading{margin:10px 0}.spui-toast--top{-webkit-transform:translate(0,-30vh);transform:translate(0,-30vh)}.spui-toast--bottom{-webkit-transform:translate(0,30vh);transform:translate(0,30vh)}