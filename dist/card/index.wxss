@import '../common/style/index.wxss';.spui-card{position:relative;display:-webkit-box;display:flex;padding:5px 15px;font-size:12px;color:#30404f;background:#fafafa;box-sizing:border-box;flex-wrap:wrap}.spui-card--center{-webkit-box-align:center;align-items:center;-webkit-box-pack:center;justify-content:center}.spui-card__thumb{position:relative;width:90px;height:90px;margin-right:10px;-webkit-box-flex:0;flex:none}.spui-card__thumb:empty{display:none}.spui-card__img{width:100%;height:100%}.spui-card__content{position:relative;height:90px;-webkit-box-flex:1;flex:1}.spui-card__desc,.spui-card__title{line-height:17px;word-break:break-all}.spui-card__title{max-height:34px;font-weight:700}.spui-card__desc{max-height:17px;color:#7d7e80}.spui-card__bottom{position:absolute;bottom:0;left:0;width:100%;line-height:17px}.spui-card__price{display:inline-block;font-weight:700;color:#f44}.spui-card__origin-price{display:inline-block;margin-left:5px;font-size:10px;color:#7d7e80;text-decoration:line-through}.spui-card__num{float:right}.spui-card__tag{position:absolute;top:2px;left:0}.spui-card__footer{width:100%;text-align:right;-webkit-box-flex:0;flex:none}