<view
  wx:if="{{ inited }}"
  class="spui-transition custom-class"
  style="-webkit-animation: spui-{{ name }}-{{ type }} {{ duration }}ms both; animation: spui-{{ name }}-{{ type }} {{ duration }}ms both; {{ display ? '' : 'display: none;' }} {{ customStyle }}"
  bind:animationend="onAnimationEnd"
>
  <slot />
</view>
